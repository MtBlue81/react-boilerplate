{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.16"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "styled-components": "5.1.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "test": "env TZ='Asia/Tokyo' react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint --ext js,jsx,ts,tsx src",
    "stylelint": "stylelint 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -s ./public -c .storybook",
    "coverage": "yarn test --coverage --watchAll=false"
  },
  "homepage": "https://MtBlue81.github.io/react-boilerplate",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.3.18",
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-docs": "5.3.18",
    "@storybook/addon-knobs": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addon-notes": "5.3.18",
    "@storybook/addon-storyshots": "5.3.18",
    "@storybook/addon-viewport": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/preset-create-react-app": "2.1.1",
    "@storybook/react": "5.3.18",
    "@storybook/source-loader": "5.3.18",
    "@testing-library/jest-dom": "5.5.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/user-event": "10.1.0",
    "@types/jest": "25.2.1",
    "@types/node": "12.12.37",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/react-helmet": "5.0.15",
    "@types/react-router-dom": "5.1.5",
    "@types/react-test-renderer": "16.9.2",
    "@types/storybook__addon-storyshots": "5.1.2",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-styled-components": "1.10.7",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prefer-arrow": "1.2.0",
    "eslint-plugin-prettier": "3.1.3",
    "gh-pages": "2.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.1",
    "prettier": "2.0.5",
    "prettier-stylelint": "0.4.2",
    "react-docgen-typescript-loader": "3.7.2",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-test-renderer": "16.13.1",
    "require-context.macro": "1.2.2",
    "stylelint": "15.10.1",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recess-order": "2.0.4",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.0.0",
    "stylelint-processor-styled-components": "1.10.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "stylelint",
      "eslint --fix"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/react-app-env.d.ts",
      "!src/serviceWorker.ts",
      "!src/index.tsx"
    ],
    "coverageReporters": [
      "text-summary"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  }
}
